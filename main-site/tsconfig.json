{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*", "src/**/*"],
  "exclude": ["dist", "node_modules", ".astro", ".vscode"],
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ]
  }
}