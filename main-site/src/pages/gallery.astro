---
import BaseLayout from "../layouts/BaseLayout.astro";
import GalleryImages from "../components/GalleryImages.svelte";
import urls from "../data/gallery.json";

const title = "Gallery";
---

<BaseLayout title={title}>
  <div class="gallery-title" style="font-size: 2rem;">
    <h1>Gallery</h1>
  </div>
  <GalleryImages urls={urls} client:load />
</BaseLayout>
